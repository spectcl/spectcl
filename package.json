{
  "name": "spectcl",
  "description": "Spawns and interacts with child processes using spawn / expect commands",
  "version": "0.7.3",
  "author": "Ryan Milbourne <ryan.milbourne@viasat.com>",
  "maintainers": [
    "Greg Cochard <greg@gregcochard.com>",
    "Ryan Milbourne <ryan.milbourne@viasat.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/spectcl/spectcl.git"
  },
  "keywords": [
    "child process",
    "expect",
    "interactive",
    "spawn",
    "terminal"
  ],
  "devDependencies": {
    "colors": "1.1.2",
    "coveralls": "^3.0.7",
    "eslint": "^4.18.2",
    "jsdoc-to-markdown": "^5.0.2",
    "lodash": "^4.0.1",
    "mocha": "^6.2.2",
    "mock-spawn": "^0.2.6",
    "nyc": "^14.1.1",
    "pre-git": "^3.17.1",
    "prompt": "^1.0.0",
    "sinon": "^1.16.1"
  },
  "main": "./lib/spectcl",
  "scripts": {
    "test": "DEBUG=spectcl mocha",
    "coverage": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha",
    "lint": "eslint lib/*.js test/*.js",
    "docugen": "./docugen.sh",
    "commit": "commit-wizard"
  },
  "engines": {
    "node": ">=8"
  },
  "license": "MIT",
  "dependencies": {
    "child_pty": "^3.0.9",
    "debug": "^2.2.0",
    "extend": "^3.0.0",
    "through": "^2.3.8"
  },
  "config": {
    "pre-git": {
      "commit-msg": [],
      "pre-commit": [
        "npm run lint"
      ],
      "pre-push": [
        "npm test"
      ],
      "post-commit": [],
      "post-merge": [
        "npm install",
        "npm test"
      ]
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
